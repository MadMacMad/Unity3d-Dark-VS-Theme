    // --- workbench: terminal
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#ff628c",
    "terminal.ansiGreen": "#3ad900",
    "terminal.ansiYellow": "#abfcff",
    "terminal.ansiBlue": "#0088ff",
    "terminal.ansiMagenta": "#fb94ff",
    "terminal.ansiCyan": "#80fcff",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiBrightBlack": "#0050A4",
    "terminal.ansiBrightRed": "#ff628c",
    "terminal.ansiBrightGreen": "#3ad900",
    "terminal.ansiBrightYellow": "#abfcff",
    "terminal.ansiBrightBlue": "#0088ff",
    "terminal.ansiBrightMagenta": "#fb94ff",
    "terminal.ansiBrightCyan": "#80fcff",
    "terminal.ansiBrightWhite": "#101820",
    "terminal.background": "#101820",
    "terminal.foreground": "#ffffff",
    "terminalCursor.background": "#abfcff",
    "terminalCursor.foreground": "#abfcff",
    // textBlockQuote
    "textBlockQuote.background": "#101820",
    "textBlockQuote.border": "#0088ff",
    "textCodeBlock.background": "#101820",
    "textLink.activeForeground": "#0088ff",
    "textLink.foreground": "#0088ff",
    "textPreformat.foreground": "#abfcff",
    "textSeparator.foreground": "#0d3a58",
    "titleBar.activeBackground": "#15232D",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#101820",
    "titleBar.inactiveForeground": "#ffffff33",
    "walkThrough.embeddedEditorBackground": "#0d3a58",
    "welcomePage.buttonBackground": "#101820",
    "welcomePage.buttonHoverBackground": "#0d3a58",
    "widget.shadow": "#00000026"
  },
  "tokenColors": [

    //Unity3d
    {
      "name": "Source",
      "scope": [
        "source"
      ],
      "settings": {
        "foreground": "#bad8bb" //new word Green
      }
    },
    {
      "name": "Keyword (operator)",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#d62828" //hell blau  = + - / %
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#a8ebe2" //Grün , ; .   a8ebe2
      }
    },
    {
      "name": "Punctuation (parenthesis)",
      "scope": [
        "punctuation.parenthesis",
        "meta.brace.round" // HOTFIX: typescript
      ],
      "settings": {
        "foreground": "#a8ebe2" // Weiß ()
      }
    },
    {
      "name": "Punctuation (square bracket)",
      "scope": [
        "punctuation.squarebracket",
        "meta.brace.square" // HOTFIX: typescript
      ],
      "settings": {
        "foreground": "#a8ebe2" // []
      }
    },
    {
      "name": "Punctuation (curly brace)",
      "scope": [
        "punctuation.curlybrace"
      ],
      "settings": {
        "foreground": "#a8ebe2" //Gelb {}
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment punctuation",
        "comment punctuation.parenthesis",
        "comment punctuation.squarebracket",
        "comment punctuation.curlybrace",
        "comment keyword",
        "comment entity",
        "comment variable",
        "comment string"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2b5c31" //Comments
      }
    },
    // Literals.
    {
      "name": "Literals (Constant)",
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "#00a800" // 00a800 Green, true false
      }
    },
    {
      "name": "Literals (null)",
      "scope": [
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#b7eaf7" //Hell blau
      } // "Literals (Constant)" - 1
    },
    {
      "name": "Literals (Number)",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#b2ffa8" //Weiß-Tükis, Zahlen Nummern
      }
    },
    {
      "name": "Literals (String)",
      "scope": [
        "string.quoted.double",
        "string.quoted.double punctuation"
      ],
      "settings": {
        "foreground": "#b4fdaac4" // b4fdaac4 "Print" Green {0:00}:{1:00}
      }
    },
    {
      "name": "Literals (Escape)",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#aa0264" //PINK \n
      } // "Literals (String)" - 1
    },
    {
      "name": "Literals (Char)",
      "scope": [
        "string.quoted.single",
        "string.quoted.single punctuation"
      ],
      "settings": {
        "foreground": "#b81790" //Lime gereen
      }
    },
    // Functions.
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "entity.name.method",
        "keyword.other.get",
        "keyword.other.set",
        "keyword.other.add",
        "keyword.other.remove",
        "support.function", // HOTFIX: typescript
        "keyword.operator.new" // HOTFIX: typescript
      ],
      "settings": {
        "foreground": "#0063f7d7" //function Names Blau Start Awake
      }
    },
    // Functions (support).
    {
      "name": "Functions (UnityEngine)",
      "scope": [
        "support.function.UnityEngine"
      ],
      "settings": {
        "foreground": "#940000" //Dunkel Rot
      } // - 1
    },
    {
      "name": "Functions (UnityEditor)",
      "scope": [
        "support.function.UnityEditor"
      ],
      "settings": {
        "foreground": "#940000" //Dunkel Rot
      } // - 1
    },
    {
      "name": "Functions (System)",
      "scope": [
        "support.function.System"
      ],
      "settings": {
        "foreground": "#940000" //Dunkel Rot
      } // - 2
    },
    // Types.
    {
      "name": "Types",
      "comment": "Including classes, delegates, enums, interfaces, structs.",
      "scope": [
        "storage.type",
        "entity.name.type.type-parameter",
        "support.type", // HOTFIX: typescript
        "support.class" // HOTFIX: typescript
      ],
      "settings": {
        "foreground": "#45b468" //Green
      }
    },
    {
      "name": "Types (built-in)",
      "scope": [
        "keyword.type"
      ],
      "settings": {
        "foreground": "#009795" //int, float, bool
      } // - 1
    },
    {
      "name": "Types (<>)",
      "scope": [
        "punctuation.definition.typeparameters"
      ],
      "settings": {
        "foreground": "#FAF600" //Hell Gelb
      } // - 2
    },
    // Types (support).
    {
      "name": "Types (UnityEngine)",
      "scope": [
        "support.type.unity.UnityEngine"
      ],
      "settings": {
        "foreground": "#4162f78a" //Mono Names KeyCode, Text, Input
      } // - 1
    },
    {
      "name": "Types (UnityEditor)",
      "scope": [
        "support.type.unity.UnityEditor"
      ],
      "settings": {
        "foreground": "#00AEAC" //Gelb
      } // - 1
    },
    {
      "name": "Types (System)",
      "scope": [
        "support.type.System"
      ],
      "settings": {
        "foreground": "#00AEAC" //Türkis
      } // - 2
    },
    // Modifiers.
    {
      "name": "Modifiers",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#d35f00" //Public etc
      } // - 2
    },
    // Types (declarations).
    {
      "name": "Types (declarations)",
      "comment": "Including classes, delegates, enums, interfaces, structs.",
      "scope": [
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#8e3d91" // Class UnityEngine
      }
    },
    // Keywords.
    {
      "name": "Keywords",
      "scope": [
        "keyword.control",
        "keyword.operator.conditional",
        "keyword.query"
      ],
      "settings": {
        "foreground": "#bd4141" //if for
      }
    },
    {
      "name": "Keywords (other)",
      "scope": [
        "keyword.other"
      ],
      "settings": {
        "foreground": "#a73d3d" //Doc Class using new
      } // - 1
    },
    // Preprocessor.
    {
      "name": "Preprocessor",
      "scope": [
        "meta.preprocessor",
        "meta.preprocessor punctuation",
        "meta.preprocessor punctuation.parenthesis",
        "meta.preprocessor punctuation.squarebracket",
        "meta.preprocessor punctuation.curlybrace",
        "meta.preprocessor keyword"
      ],
      "settings": {
        "foreground": "#FF0000" //Red
      } // - 2
    },
    {
      "name": "Preprocessor (entities)",
      "scope": [
        "meta.preprocessor entity",
        "meta.preprocessor entity.name.method",
        "meta.preprocessor entity.name.function",
        "meta.preprocessor entity.name.variable",
        "meta.preprocessor variable",
        "meta.preprocessor string"
      ],
      "settings": {
        "foreground": "#FF0000" //Red
      } // - 1
    },
    // Variables.
    {
      "name": "Variables",
      "scope": [
        "variable.other.readwrite",
        "variable.other.object",
        "variable.other.alias",
        "entity.name.variable",
        "variable.other.property", // HOTFIX: typescript
        "variable.other.constant", // HOTFIX: typescript
        "variable.parameter", // HOTFIX: typescript
        "variable.object.property", // HOTFIX: typescript
        "variable.object.constant", // HOTFIX: typescript
        "meta.object-literal.key", // HOTFIX: typescript
        "support.variable" // HOTFIX: typescript
      ],
      "settings": {
        "foreground": "#919191" //Standart Font Color..
      }
    },
    {
      "name": "Variables (local)",
      "scope": [
        "variable.other.object.property",
        "entity.name.variable.local",
        "keyword.other.base",
        "keyword.other.this",
        "variable.language.base", // HOTFIX: typescript
        "variable.language.this" // HOTFIX: typescript
      ],
      "settings": {
        "foreground": "#7f8100ee" //Hell Grün -Position, size
      } // - 1
    },
    // Variables (support).
    {
      "name": "Variables (UnityEngine)",
      "scope": [
        "support.variable.UnityEngine"
      ],
      "settings": {
        "foreground": "#C96900"
      } // - 1
    },
    {
      "name": "Variables (UnityEditor)",
      "scope": [
        "support.variable.UnityEditor"
      ],
      "settings": {
        "foreground": "#C96900"
      } // - 1
    },
    {
      "name": "Variables (System)",
      "scope": [
        "support.variable.System"
      ],
      "settings": {
        "foreground": "#C96900"
      } // - 2
    },

/////////////////////////////////////////////////////////////////////////










