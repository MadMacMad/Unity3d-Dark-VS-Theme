    "input.placeholderForeground": "#c2ddc2",
    "inputOption.activeBorder": "#0d3a58",
    "inputValidation.errorBackground": "#101820",
    "inputValidation.errorBorder": "#ffc600dd",
    "inputValidation.infoBackground": "#101820",
    "inputValidation.infoBorder": "#0D3A58",
    "inputValidation.warningBackground": "#101820",
    "inputValidation.warningBorder": "#abfcff",
    // list
